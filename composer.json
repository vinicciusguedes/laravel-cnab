{
    "name": "vinicciusguedes/laravel-cnab",
    "description": "Biblioteca para gerar boletos, remessas e leitura de retorno em laravel",
    "keywords": [
        "vinicciusguedes",
        "laravel-cnab"
    ],
    "homepage": "https://github.com/vinicciusguedes/laravel-cnab",
    "license": "MIT",
    "authors": [
        {
            "name": "VinÃ­ccius Guedes",
            "email": "vgndevcode@gmail.com"
        }
    ],
    "require": {
        "php":                ">5.5.0",
        "ext-intl":           "*",
        "ext-mbstring":       "*",
        "laravel/framework":  "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "neitanod/forceutf8": "^2.0",
        "setasign/fpdf":      "^1.8",
        "ext-curl":           "*",
        "ext-json":           "*",
        "ext-openssl":        "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "orchestra/testbench": "^3.0|^4.0|^5.0|^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "VinicciusGuedes\\LaravelCnab\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VinicciusGuedes\\LaravelCnab\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": true
    }
}